{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/NodeData",
    "definitions": {
        "NodeData": {
            "properties": {
                "staked_total": {
                    "type": "string",
                    "description": "Total staked amount across all nodes"
                },
                "total_nodes": {
                    "type": "integer",
                    "description": "Total number of nodes"
                },
                "inactive_nodes": {
                    "type": "integer",
                    "description": "Number of inactive nodes"
                },
                "validating_nodes": {
                    "type": "integer",
                    "description": "Number of nodes validating"
                },
                "uptime": {
                    "type": "number",
                    "description": "Total uptime for all epochs across all nodes"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Node Data"
        }
    }
}