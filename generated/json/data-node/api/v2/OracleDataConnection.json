{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/OracleDataConnection",
    "definitions": {
        "OracleDataConnection": {
            "properties": {
                "edges": {
                    "items": {
                        "$ref": "#/definitions/datanode.api.v2.OracleDataEdge"
                    },
                    "type": "array"
                },
                "page_info": {
                    "$ref": "#/definitions/datanode.api.v2.PageInfo",
                    "additionalProperties": true
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Oracle Data Connection"
        },
        "datanode.api.v2.OracleDataEdge": {
            "properties": {
                "node": {
                    "$ref": "#/definitions/vega.data.v1.ExternalData",
                    "additionalProperties": true
                },
                "cursor": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Oracle Data Edge"
        },
        "datanode.api.v2.PageInfo": {
            "properties": {
                "has_next_page": {
                    "type": "boolean"
                },
                "has_previous_page": {
                    "type": "boolean"
                },
                "start_cursor": {
                    "type": "string"
                },
                "end_cursor": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Page Info",
            "description": "Page information for cursor based pagination"
        },
        "vega.data.v1.ExternalData": {
            "properties": {
                "signers": {
                    "items": {
                        "$ref": "#/definitions/vega.data.v1.Signer"
                    },
                    "type": "array"
                },
                "data": {
                    "items": {
                        "$ref": "#/definitions/vega.data.v1.Property"
                    },
                    "type": "array",
                    "description": "data holds all the properties of the data"
                },
                "matched_spec_ids": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array",
                    "description": "matched_specs_ids lists all the specs that matched this data. When the array is empty, it means no spec matched this data."
                },
                "broadcast_at": {
                    "type": "string",
                    "description": "broadcast_at is the time at which the data was broadcast to the markets with a matching spec. It has no value when the date did not match any spec. The value is a Unix timestamp in nanoseconds."
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "External Data",
            "description": "ExternalData describes valid external source data that has been received by the node. It represents both matched and unmatched data."
        },
        "vega.data.v1.Property": {
            "properties": {
                "name": {
                    "type": "string",
                    "description": "name is the name of the property."
                },
                "value": {
                    "type": "string",
                    "description": "value is the value of the property."
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Property",
            "description": "Property describes one property of an oracle spec with a key with its value."
        },
        "vega.data.v1.Signer": {
            "properties": {
                "pub_key": {
                    "type": "string",
                    "description": "pubKeys is the list of authorized public keys that signed the data for this source. All the public keys in the data should be contained in these public keys."
                },
                "address": {
                    "type": "string",
                    "description": "in case of an open oracle - Ethereum address will be submitted"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Signer"
        }
    }
}